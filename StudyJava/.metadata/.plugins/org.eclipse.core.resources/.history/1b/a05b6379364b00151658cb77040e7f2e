package com.jingucao.thread1;

public class MyThread {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Thread t = new ThreadA();
		t.start();
		
		while(true)
			System.out.print(3);

	}

}

class ThreadA extends Thread{
	public void run(){
		while(true)
			System.out.println(2);
	}
	
	
	
}